{
    "build": {
        "beforeDevCommand": "trunk serve",
        "beforeBuildCommand": "trunk build",
        "devPath": "http://127.0.0.1:10270",
        "distDir": "../dist",
        "withGlobalTauri": true
    },
    "tauri": {
        "allowlist": {
            "window": {
                "startDragging": true,
                "maximize": true,
                "unmaximize": true
            }
        },
        "windows": [
            {
                "title": "标题",
                "width": 1050,
                "height": 750,
                "decorations": false,
                "transparent": true
            }
        ],
        "bundle": {
            "identifier": "com.zhangxichang.demo",
            "icon": [
                "./icon.ico"
            ]
        }
    }
}
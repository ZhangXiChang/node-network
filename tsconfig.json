{
    "compilerOptions": {
        //JSX配置
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "types": [
            "vite/client"
        ],
        //目标配置
        "noEmit": true,
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "isolatedModules": true,
        //规则配置
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
    },
}